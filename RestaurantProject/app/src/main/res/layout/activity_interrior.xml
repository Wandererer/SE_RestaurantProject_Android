<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:paddingTop="10dp"
    tools:context="com.example.jk.restaurantproject.InterriorActivity">



    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/topFragment"
        android:name="com.example.jk.restaurantproject.InterriorTopSectionFragment"
        tools:layout="@layout/button_fragmentlayout"
        />

    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/topFragment"
        android:name="com.example.jk.restaurantproject.InterriorBottomSectionFragment"
        android:id="@+id/bottomFragment"
        tools:layout="@layout/interrior_fragmentlayout"
        />



</RelativeLayout>
